<div class='col-sm-6 col-lg-4'>
  <div class="card">
    <div class="card-header text-center">
      Mapa
    </div>
    <%= image_tag "http://maps.google.com/maps/api/staticmap?size=350x200&sensor=false&zoom=15&markers=#{place.latitude}%2C#{place.longitude}", :class => "card-img-top" %>
    <div class="card-block">
      <!-- <h4 class="card-title"></h4> -->
      <p class="card-text"><%= place.descricao %></p>
      <p class="card-text">Lat: <%= place.latitude %>, Long: <%= place.longitude %></p>
    </div>
  </div>
  <br>
</div>
